<?xml version="1.0"?>
<EmbeddedProjectSample xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>LEDBlink</Name>
	<Description>This is a very basic sample project - a classical blinking LED example.</Description>
	<MCUFilterRegex>^XMC1.*</MCUFilterRegex>
<ConfigurableProperties>
	<PropertyGroups>
	<PropertyGroup>
	  <Properties>
		<PropertyEntry xsi:type="Enumerated">
		  <Name>LED Port Group</Name>
		  <UniqueID>com.sysprogs.examples.ledblink.LEDPORT</UniqueID>
		  <Description>Specifies the port group where the LED is connected</Description>
		  <SuggestionList>
			<Suggestion><UserFriendlyName>P0</UserFriendlyName><InternalValue>0</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>P1</UserFriendlyName><InternalValue>1</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>P2</UserFriendlyName><InternalValue>2</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>P3</UserFriendlyName><InternalValue>3</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>P4</UserFriendlyName><InternalValue>4</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>P5</UserFriendlyName><InternalValue>5</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>P6</UserFriendlyName><InternalValue>6</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>P7</UserFriendlyName><InternalValue>7</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>P8</UserFriendlyName><InternalValue>8</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>P9</UserFriendlyName><InternalValue>9</InternalValue></Suggestion>
		  </SuggestionList>
		  <DefaultEntryIndex>0</DefaultEntryIndex>
		  <AllowFreeEntry>false</AllowFreeEntry>
		</PropertyEntry>
		<PropertyEntry xsi:type="Integral">
		  <Name>LED Port Number</Name>
		  <UniqueID>com.sysprogs.examples.ledblink.LEDBIT</UniqueID>
		  <Description>Specifies the port number of the LED port</Description>
		  <DefaultValue>6</DefaultValue>
		  <MinValue>0</MinValue>
		  <MaxValue>15</MaxValue>
		</PropertyEntry>
		<PropertyEntry xsi:type="Integral">
		  <Name>Delay (msec)</Name>
		  <UniqueID>com.sysprogs.examples.ledblink.DELAYMSEC</UniqueID>
		  <Description>Specifies the delay (in milliseconds) between LED toggling events</Description>
		  <DefaultValue>500</DefaultValue>
		</PropertyEntry>
	  </Properties>
	  <CollapsedByDefault>false</CollapsedByDefault>
	</PropertyGroup>
	</PropertyGroups>
	</ConfigurableProperties>
</EmbeddedProjectSample>