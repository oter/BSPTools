<?xml version="1.0"?>
<FamilyDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>LPC11xxM0PLUS</Name>
	<DeviceRegex>^LPC11.[6].*</DeviceRegex>
	<FamilySubdirectory>LPC11u6x</FamilySubdirectory>
	<PrimaryHeaderDir>$$BSPGEN:INPUT_DIR$$</PrimaryHeaderDir>
	<StartupFileDir>$$BSPGEN:INPUT_DIR$$\lpc11u6x\startup_code</StartupFileDir>
	<Subfamilies>		
	</Subfamilies>	
	<ConfigurableProperties>
        <PropertyGroups>
          <PropertyGroup>
           </PropertyGroup>
        </PropertyGroups>
      </ConfigurableProperties>
	  	<CoreFramework>
