				<CopyJob>
					<SourceFolder>$$BSPGEN:INPUT_DIR$$\lpc11u6x</SourceFolder>
					<FilesToCopy>-*sys_config.h;-*startup_code\*;*.h;*.c</FilesToCopy>
					<TargetFolder>lpc_chip</TargetFolder>
					<PreprocessorMacros>CHIP_LPC11U6X;CORE_M0PLUS</PreprocessorMacros>	
